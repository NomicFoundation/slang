{
  "ignorePatterns": ["CHANGELOG.md"],
  "replacementPatterns": [],
  "httpHeaders": [
    {
      "__comment__": "More Info: https://github.com/rust-lang/crates.io/issues/788",
      "urls": ["https://crates.io/"],
      "headers": {
        "Accept": "text/html"
      }
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 5,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 204, 206]
}
