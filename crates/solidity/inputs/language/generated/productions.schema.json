{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Array_of_Production",
  "type": "array",
  "items": {
    "$ref": "#/definitions/Production"
  },
  "definitions": {
    "Production": {
      "type": "object",
      "anyOf": [
        {
          "type": "object",
          "anyOf": [
            {
              "type": "object",
              "required": ["unversioned"],
              "properties": {
                "unversioned": {
                  "$ref": "#/definitions/Scanner"
                }
              }
            },
            {
              "type": "object",
              "required": ["versioned"],
              "properties": {
                "versioned": {
                  "type": "object",
                  "additionalProperties": {
                    "anyOf": [
                      {
                        "$ref": "#/definitions/Scanner"
                      },
                      {
                        "type": "null"
                      }
                    ]
                  }
                }
              }
            }
          ],
          "required": ["kind"],
          "properties": {
            "kind": {
              "type": "string",
              "enum": ["Scanner"]
            }
          }
        },
        {
          "type": "object",
          "anyOf": [
            {
              "type": "object",
              "required": ["unversioned"],
              "properties": {
                "unversioned": {
                  "$ref": "#/definitions/Parser"
                }
              }
            },
            {
              "type": "object",
              "required": ["versioned"],
              "properties": {
                "versioned": {
                  "type": "object",
                  "additionalProperties": {
                    "anyOf": [
                      {
                        "$ref": "#/definitions/Parser"
                      },
                      {
                        "type": "null"
                      }
                    ]
                  }
                }
              }
            }
          ],
          "required": ["kind"],
          "properties": {
            "kind": {
              "type": "string",
              "enum": ["TriviaParser"]
            }
          }
        },
        {
          "type": "object",
          "anyOf": [
            {
              "type": "object",
              "required": ["unversioned"],
              "properties": {
                "unversioned": {
                  "$ref": "#/definitions/Parser"
                }
              }
            },
            {
              "type": "object",
              "required": ["versioned"],
              "properties": {
                "versioned": {
                  "type": "object",
                  "additionalProperties": {
                    "anyOf": [
                      {
                        "$ref": "#/definitions/Parser"
                      },
                      {
                        "type": "null"
                      }
                    ]
                  }
                }
              }
            }
          ],
          "required": ["kind"],
          "properties": {
            "kind": {
              "type": "string",
              "enum": ["Parser"]
            }
          }
        },
        {
          "type": "object",
          "anyOf": [
            {
              "type": "object",
              "required": ["unversioned"],
              "properties": {
                "unversioned": {
                  "$ref": "#/definitions/PrecedenceParser"
                }
              }
            },
            {
              "type": "object",
              "required": ["versioned"],
              "properties": {
                "versioned": {
                  "type": "object",
                  "additionalProperties": {
                    "anyOf": [
                      {
                        "$ref": "#/definitions/PrecedenceParser"
                      },
                      {
                        "type": "null"
                      }
                    ]
                  }
                }
              }
            }
          ],
          "required": ["kind"],
          "properties": {
            "kind": {
              "type": "string",
              "enum": ["PrecedenceParser"]
            }
          }
        }
      ],
      "required": ["name"],
      "properties": {
        "name": {
          "type": "string"
        },
        "inlined": {
          "default": false,
          "type": "boolean"
        }
      }
    },
    "Scanner": {
      "type": "object",
      "anyOf": [
        {
          "title": "Choice Expression",
          "type": "object",
          "required": ["choice"],
          "properties": {
            "choice": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Scanner"
              }
            }
          }
        },
        {
          "title": "Difference Expression",
          "type": "object",
          "required": ["difference"],
          "properties": {
            "difference": {
              "type": "object",
              "required": ["minuend", "subtrahend"],
              "properties": {
                "minuend": {
                  "$ref": "#/definitions/Scanner"
                },
                "subtrahend": {
                  "$ref": "#/definitions/Scanner"
                }
              }
            }
          }
        },
        {
          "title": "Not Expression",
          "type": "object",
          "required": ["not"],
          "properties": {
            "not": {
              "$ref": "#/definitions/Scanner"
            }
          }
        },
        {
          "title": "OneOrMore Expression",
          "type": "object",
          "required": ["oneOrMore"],
          "properties": {
            "oneOrMore": {
              "$ref": "#/definitions/Scanner"
            }
          }
        },
        {
          "title": "Optional Expression",
          "type": "object",
          "required": ["optional"],
          "properties": {
            "optional": {
              "$ref": "#/definitions/Scanner"
            }
          }
        },
        {
          "title": "Range Expression",
          "type": "object",
          "required": ["range"],
          "properties": {
            "range": {
              "type": "object",
              "required": ["from", "to"],
              "properties": {
                "from": {
                  "type": "string",
                  "maxLength": 1,
                  "minLength": 1
                },
                "to": {
                  "type": "string",
                  "maxLength": 1,
                  "minLength": 1
                }
              }
            }
          }
        },
        {
          "title": "Reference Expression",
          "type": "object",
          "required": ["reference"],
          "properties": {
            "reference": {
              "type": "string"
            }
          }
        },
        {
          "title": "Sequence Expression",
          "type": "object",
          "required": ["sequence"],
          "properties": {
            "sequence": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Scanner"
              }
            }
          }
        },
        {
          "title": "TrailingContext Expression",
          "type": "object",
          "required": ["trailingContext"],
          "properties": {
            "trailingContext": {
              "type": "object",
              "required": ["expression", "notFollowedBy"],
              "properties": {
                "expression": {
                  "$ref": "#/definitions/Scanner"
                },
                "notFollowedBy": {
                  "$ref": "#/definitions/Scanner"
                }
              }
            }
          }
        },
        {
          "title": "Terminal Expression",
          "type": "object",
          "required": ["terminal"],
          "properties": {
            "terminal": {
              "type": "string"
            }
          }
        },
        {
          "title": "ZeroOrMore Expression",
          "type": "object",
          "required": ["zeroOrMore"],
          "properties": {
            "zeroOrMore": {
              "$ref": "#/definitions/Scanner"
            }
          }
        }
      ]
    },
    "Parser": {
      "type": "object",
      "anyOf": [
        {
          "title": "Choice Expression",
          "type": "object",
          "required": ["choice"],
          "properties": {
            "choice": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Parser"
              }
            }
          }
        },
        {
          "title": "DelimitedBy Expression",
          "type": "object",
          "required": ["delimitedBy"],
          "properties": {
            "delimitedBy": {
              "type": "object",
              "required": ["close", "expression", "open"],
              "properties": {
                "open": {
                  "$ref": "#/definitions/Reference"
                },
                "expression": {
                  "$ref": "#/definitions/Parser"
                },
                "close": {
                  "$ref": "#/definitions/Reference"
                }
              }
            }
          }
        },
        {
          "title": "OneOrMore Expression",
          "type": "object",
          "required": ["oneOrMore"],
          "properties": {
            "oneOrMore": {
              "$ref": "#/definitions/Parser"
            }
          }
        },
        {
          "title": "Optional Expression",
          "type": "object",
          "required": ["optional"],
          "properties": {
            "optional": {
              "$ref": "#/definitions/Parser"
            }
          }
        },
        {
          "title": "Reference Expression",
          "type": "object",
          "required": ["reference"],
          "properties": {
            "reference": {
              "type": "string"
            }
          }
        },
        {
          "title": "SeparatedBy Expression",
          "type": "object",
          "required": ["separatedBy"],
          "properties": {
            "separatedBy": {
              "type": "object",
              "required": ["expression", "separator"],
              "properties": {
                "expression": {
                  "$ref": "#/definitions/Parser"
                },
                "separator": {
                  "$ref": "#/definitions/Reference"
                }
              }
            }
          }
        },
        {
          "title": "Sequence Expression",
          "type": "object",
          "required": ["sequence"],
          "properties": {
            "sequence": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Parser"
              }
            }
          }
        },
        {
          "title": "TerminatedBy Expression",
          "type": "object",
          "required": ["terminatedBy"],
          "properties": {
            "terminatedBy": {
              "type": "object",
              "required": ["expression", "terminator"],
              "properties": {
                "expression": {
                  "$ref": "#/definitions/Parser"
                },
                "terminator": {
                  "$ref": "#/definitions/Reference"
                }
              }
            }
          }
        },
        {
          "title": "ZeroOrMore Expression",
          "type": "object",
          "required": ["zeroOrMore"],
          "properties": {
            "zeroOrMore": {
              "$ref": "#/definitions/Parser"
            }
          }
        }
      ]
    },
    "Reference": {
      "type": "object",
      "required": ["reference"],
      "properties": {
        "reference": {
          "title": "Production Reference",
          "type": "string"
        }
      }
    },
    "PrecedenceParser": {
      "type": "object",
      "required": ["operators", "primaryExpression"],
      "properties": {
        "operators": {
          "title": "Operator Definitions",
          "type": "array",
          "items": {
            "$ref": "#/definitions/OperatorDefinition"
          }
        },
        "primaryExpression": {
          "title": "Primary Expression",
          "anyOf": [
            {
              "$ref": "#/definitions/Parser"
            }
          ]
        }
      }
    },
    "OperatorDefinition": {
      "type": "object",
      "required": ["model", "name", "operator"],
      "properties": {
        "name": {
          "type": "string"
        },
        "model": {
          "$ref": "#/definitions/OperatorModel"
        },
        "operator": {
          "$ref": "#/definitions/Parser"
        }
      }
    },
    "OperatorModel": {
      "type": "string",
      "enum": ["BinaryLeftAssociative", "BinaryRightAssociative", "UnaryPrefix", "UnaryPostfix"]
    }
  }
}
