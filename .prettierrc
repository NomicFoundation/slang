{
  "bracketSpacing": true,
  "endOfLine": "lf",
  "printWidth": 120,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "tabWidth": 2,
  "plugins": ["prettier-plugin-sh"],
  "overrides": [
    {
      "files": "**/*.md",
      "options": {
        "tabWidth": 4
      }
    }
  ]
}
