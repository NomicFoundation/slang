{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/v5.19.2/cspell.schema.json",
  "language": "en-US",
  "files": ["**/*.md"],
  "ignorePaths": ["**/generated/**"],
  "ignoreWords": [
    // Alphabetically sorted list of "allowed" words to ignore:
    "abicoder",
    "codegen",
    "devcontainer",
    "doxygen",
    "inheritdoc",
    "ipfs",
    "nomic",
    "struct",
    "structs",
    "ufixed"
  ]
}
